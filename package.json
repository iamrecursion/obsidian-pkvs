{
  "name": "obsidian-pkvs",
  "version": "1.1.0",
  "description": "Provides a persistent key-value store for use in scripts in Obsidian.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "postinstall": "husky install; npx husky add .husky/pre-commit \"npx lint-staged\""
  },
  "keywords": [],
  "author": "Ara Adkins",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.10.6",
    "@types/serialize-javascript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.19.11",
    "obsidian": "latest",
    "prettier": "^3.1.1",
    "serialize-javascript": "^6.0.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "husky": "^7.0.2",
    "lint-staged": "^10.5.3"
  },
  "dependencies": {
    "eruda": "^3.0.1",
    "eruda-benchmark": "^2.0.1",
    "eruda-code": "^2.1.0",
    "eruda-features": "^2.0.0",
    "eruda-geolocation": "^2.0.0",
    "eruda-monitor": "^1.0.0",
    "eruda-orientation": "^2.0.0",
    "eruda-timing": "^2.0.1",
    "eruda-touches": "^2.0.0"
  },
  "lint-staged": {
    "*.md": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.yaml": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write"
    ]
  }
}
